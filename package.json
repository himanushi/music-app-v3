{
  "name": "music-app",
  "version": "0.0.1",
  "description": "music-app",
  "main": "index.js",
  "scripts": {
    "start": "run-p route dev",
    "dev": "snowpack dev",
    "ios:build": "ionic capacitor build ios",
    "ios:live": "ionic capacitor run ios --livereload-url=http://localhost:8080",
    "ios:run": "ionic capacitor run ios",
    "build": "rm -fr build && routify -b && snowpack build && . scripts/cache-buster.bash && . scripts/rename-index.bash",
    "origin:build": "rm -fr build && routify -b && snowpack build",
    "lint": "prettier --write . && eslint --fix --ext .js,.ts . && eslint --fix --ext .svelte .",
    "route": "routify -b",
    "routing": "routify",
    "generate": "npx graphql-codegen --config=codegen.yml",
    "ionic:build": "run-s origin:build",
    "ionic:serve": "run-s start"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@capacitor/cli": "^3.2.0",
    "@capacitor/core": "^3.2.0",
    "@capacitor/ios": "^3.2.0",
    "@ionic/core": "^6.0.12",
    "@ionic/storage": "^3.0.6",
    "@types/howler": "^2.2.2",
    "@xstate/inspect": "^0.4.1",
    "@xstate/svelte": "^0.1.0",
    "axios": "^0.21.1",
    "capacitor-plugin-applemusic": "^0.0.36",
    "dompurify": "^2.2.8",
    "graphql": "^15.5.0",
    "howler": "^2.2.1",
    "js-cookie": "^2.2.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.merge": "^4.6.2",
    "lodash.mergewith": "^4.6.2",
    "marked": "^2.0.5",
    "svelte": "^3.35.0",
    "uuid": "^8.3.2",
    "xstate": "^4.17.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.2",
    "@graphql-codegen/typed-document-node": "^1.18.3",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@roxi/routify": "^2.18.0",
    "@snowpack/plugin-svelte": "^3.5.2",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@tsconfig/svelte": "^2.0.1",
    "@types/dompurify": "^2.2.2",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.168",
    "@types/marked": "^2.0.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-svelte3": "^3.1.2",
    "ismobilejs": "^1.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "snowpack": "^3.5.1",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "svelte-apollo": "^0.4.0",
    "svelte-preprocess": "^4.6.9",
    "svelte-range-slider-pips": "^1.7.0",
    "typescript": "^4.2.3"
